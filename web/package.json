{
  "name": "kviss-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:css\" \"API_URL=http://localhost:8081/api WS_URL=http://localhost:8081 vite\"",
    "build": "npm run build:css && tsc && vite build",
    "preview": "vite preview",
    "build:css": "npx @tailwindcss/cli -m -i ./app/main.css -o ./app/styles/app.css",
    "dev:css": "npx @tailwindcss/cli -w -i ./app/main.css -o ./app/styles/app.css"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.2.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.10",
    "@types/node": "^22.13.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.4",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.8",
    "typescript": "^5.8.2",
    "vite": "^6.2.0"
  },
  "engines": {
    "node": ">=22"
  }
}
